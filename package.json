    "nodes": [
      "dist/nodes/NextcloudDeck/NextcloudDeck.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.0",
    "gulp": "^5.0.0",
    "n8n-workflow": "^1.2.0",
    "prettier": "^3.0.0",
    "typescript": "~5.3.0"
